.data
_nl: .asciiz "\n"
.text
.globl println
println:
	addiu $sp, $sp, -8
	sw $fp, 4($sp)
	sw $ra, 0($sp)
	move $fp, $sp
	lw   $a0, 8($fp)
	li   $v0, 1
	syscall
	la   $a0, _nl
	li   $v0, 4
	syscall
	move $sp, $fp
	lw $ra, 0($sp)
	lw $fp, 4($sp)
	addiu $sp, $sp, 8
jr   $ra

.globl perro
perro:
    addiu $sp, $sp, -16    # tmps: 0, params: 2, 2
    sw $fp, 4($sp)         # Save the old frame pointer
    sw $ra, 0($sp)         # Save the return address
    move $fp, $sp          # Set the new frame pointer

    lw $t0, 8($fp)
    la $sp, -4($sp)
    sw $t0, 0($sp)

    jal println
    la $sp, 4($sp)

    lw $t0, 12($fp)
    la $sp, -4($sp)
    sw $t0, 0($sp)

    jal println
    la $sp, 4($sp)


    move $sp, $fp
    lw $ra, 0($sp)
    lw $fp, 4($sp)
    addiu $sp, $sp, 16
jr $ra

.globl main
main:
    addiu $sp, $sp, -20    # tmps: 3, params: 0, 0
    sw $fp, 4($sp)         # Save the old frame pointer
    sw $ra, 0($sp)         # Save the return address
    move $fp, $sp          # Set the new frame pointer

    li $t0, 1234
    la $sp, -4($sp)
    sw $t0, 0($sp)

    jal println
    la $sp, 4($sp)

    li $t0, 2
    la $sp, -4($sp)
    sw $t0, 0($sp)

    li $t0, 1
    la $sp, -4($sp)
    sw $t0, 0($sp)

    jal perro
    la $sp, 8($sp)


    move $sp, $fp
    lw $ra, 0($sp)
    lw $fp, 4($sp)
    addiu $sp, $sp, 20
jr $ra

