.data
_nl: .asciiz "\n"
.text
.globl println
println:
li   $v0, 1
syscall
la   $a0, _nl
li   $v0, 4
syscall
jr   $ra

.globl perro
perro:
    addiu $sp, $sp, -24    # tmps: 0, params: 4, 4
    sw $fp, 4($sp)         # Save the old frame pointer
    sw $ra, 0($sp)         # Save the return address
    move $fp, $sp          # Set the new frame pointer

    var: x
    add $a0, $zero, $t0
    jal println

    move $sp, $fp
    lw $ra, 0($sp)
    lw $fp, 4($sp)
    addiu $sp, $sp, 24
jr $ra

.globl main
main:
    addiu $sp, $sp, -28    # tmps: 5, params: 0, 0
    sw $fp, 4($sp)         # Save the old frame pointer
    sw $ra, 0($sp)         # Save the return address
    move $fp, $sp          # Set the new frame pointer

    li $t0, 1234
    add $a0, $zero, $t0
    jal println
    li $t0, 1
    add $a0, $zero, $t0
    li $t0, 2
    add $a0, $zero, $t0
    li $t0, 3
    add $a0, $zero, $t0
    li $t0, 4
    add $a0, $zero, $t0
    jal perro

    move $sp, $fp
    lw $ra, 0($sp)
    lw $fp, 4($sp)
    addiu $sp, $sp, 28
jr $ra

